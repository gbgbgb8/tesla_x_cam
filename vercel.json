{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
          { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
          { "key": "Content-Security-Policy", "value": "default-src 'self'; script-src 'self' https://unpkg.com 'unsafe-eval' 'wasm-unsafe-eval'; worker-src blob:;" }
        ]
      }
    ]
  }